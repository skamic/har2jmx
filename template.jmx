          <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="endTime" enabled="true">
            <stringProp name="BeanShellSampler.query">endTime = System.currentTimeMillis();

requestInterval = ${REQ_INTERVAL};
elapsedTime = endTime - Long.parseLong(vars.get(&quot;startTime&quot;));
pauseTime = (elapsedTime &gt; requestInterval ? 0 : requestInterval - elapsedTime);

vars.put(&quot;pauseTime&quot;, String.valueOf(pauseTime));

log.error(&quot;endTime: &quot; + endTime);
log.error(&quot;elapsedTime: &quot; + elapsedTime);
log.error(&quot;pauseTime: &quot;, vars.get(&quot;pauseTime&quot;));</stringProp>
            <stringProp name="BeanShellSampler.filename"></stringProp>
            <stringProp name="BeanShellSampler.parameters"></stringProp>
            <boolProp name="BeanShellSampler.resetInterpreter">false</boolProp>
          </BeanShellSampler>
